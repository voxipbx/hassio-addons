{
  "name": "SSH Tunnel",
  "version": "1.0.1",
  "slug": "ssh_tunnel",
  "description": "Create SSH tunnel to remote controlled server",
  "url": "https://github.com/VoxIPBX/hassio-addons/ssh_tunnel",
  "arch": ["armv7", "armhf", "amd64", "aarch64", "i386"],
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "options": {
    "hostname": "remotessh.voxip.nl",
    "ssh_port": 22,
    "username": "public",
    "key": "public",
    "remote_forwarding": ["127.0.0.1:10081:192.168.10.10:8123"],
    "other_ssh_options": "-v ",
    "force_keygen": false,
    "usernote": ""
  },
  "schema": {
    "hostname": "str",
    "ssh_port": "int",
    "username": "str",
    "key": "str",
    "remote_forwarding": ["str"],
    "other_ssh_options": "str",
    "force_keygen": "bool",
    "usernote": "str"
  }
}
