{
  "name": "SSH Tunnel",
  "version": "1.0.1",
  "slug": "ssh_tunnel",
  "description": "Create SSH tunnel to remote controlled server",
  "url": "https://github.com/VoxIPBX/hassio-addons/ssh_tunnel",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "startup": "system",
  "boot": "auto",
  "host_network": true,
  "options": {
    "ssh_host": "remotessh.voxip.nl",
    "ssh_user": "root",
    "ssh_port": 22,
    "tunnel_http": true,
    "tunnel_https": true,
    "socks_port": 10081
  },
  "schema": {
    "ssh_host": "str",
    "ssh_user": "str",
    "ssh_port": "int",
    "tunnel_http": "bool",
    "tunnel_https": "bool",
    "socks_port": "int",
    "advanced": "str?",
    "before": "str?"
  }
}
